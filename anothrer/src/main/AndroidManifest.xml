<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.videoview">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TestKotlin">
        <activity
            android:name=".ToolBarActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NavigationViewActivity"
            android:exported="true">

        </activity>
        <activity
            android:name=".AppBarActivity"
            android:exported="false" />
        <activity
            android:name=".AppBar2Activity"
            android:exported="true"></activity>
        <activity
            android:name=".NestedScrollViewActivity"
            android:exported="true" />
        <activity
            android:name=".SpinnerActivity"
            android:exported="true" />
        <activity
            android:name=".TableActivity"
            android:exported="false" />
        <activity
            android:name=".FrameActivity"
            android:exported="false" />
        <activity
            android:name=".LinearActivity"
            android:exported="false" />
        <activity
            android:name=".ConstraintActivity"
            android:exported="false" />
        <activity
            android:name=".SearchViewActivity"
            android:exported="true" />
        <activity
            android:name=".RatingBarActivity"
            android:exported="true" />
        <activity
            android:name=".SeekBarActivity"
            android:exported="true" />
        <activity
            android:name=".PrograssBarActivity"
            android:exported="true" />
    </application>

</manifest>